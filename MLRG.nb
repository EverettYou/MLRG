Cell["\<\
up, seg=2.4, 0.01
N=5
mlrg=MLRG(Sn_dat[2],['A1'],[6,12,8]).to(device)
mlrg.load_state_dict(torch.load(<*NotebookDirectory[]*> + \"/models/A1/\" + \
\"1mlrg_A1.pth\", map_location=torch.device('cpu')))
mlrg.eval()
rbm = EquivariantRBM(biadj_dat['cross'], Bond(Sn_dat[2], ['A1']))
J=torch.arange(0.,up,seg).view(-1,1)
JA=J.clone()
Gs=[]
m=[]
for _ in range(N):
\trbm.set_J(J)
\tG = GSD(rbm.prob_src(split=True)).view(-1,1)
\tGs.append(torch.cat((JA,G),-1))
\tm.append(torch.cat((JA, mlrg.moderator(J).view(-1,1)),1))
\tJ=mlrg.moderator.RGforward(J)
m=torch.cat(m).view(N,-1,2)
Gs=torch.cat(Gs).view(N,-1,2)\
\>", "ExternalLanguage",
