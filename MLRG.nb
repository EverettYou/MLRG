 FormBox[
  RowBox[{
   SubscriptBox["\[DoubleStruckCapitalT]", 
    RowBox[{
     SubscriptBox["x", "1"], 
     SubscriptBox["x", "2"], 
     SubscriptBox["x", "3"], 
     SubscriptBox["x", "4"]}]], "\[Proportional]", 
   RowBox[{
    UnderscriptBox["\[Sum]", 
