Cell["\<\
J=torch.tensor([[1.6,0.,0.,0.,0.]])
#J=mlrg.moderator.RGforward(J)
scals=[]
for _ in range(100):
\tJ=mlrg.moderator.Newton_step(J,step_size=0.1)
\trbm.set_J(J)
\tvals=Tvals(rbm.prob_src(split=True), power=1)
\tspec = -vals.abs().log()/(2*torch.pi)
\tspec -= spec.min(-1, keepdim=True)[0]
\tscals.append(spec[-2].view(-1))
print(J,scals[-1])
scals=torch.cat(scals)\
\>", "ExternalLanguage",
