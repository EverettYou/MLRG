Cell["\<\
up, seg=2.4, 0.01
N=5
mlrg=MLRG(Sn_dat[2],['A1','E'],[12,24,6]).to(device)
mlrg.load_state_dict(torch.load(<*NotebookDirectory[]*> + \"/models/A1E/\" + \
\"trial0/8mlrg_A1E.pth\", map_location=torch.device('cpu')))
mlrg.eval()
JA=torch.arange(0.,up,seg).view(-1,1)
J=torch.zeros(JA.shape[0],5)
J[...,0]+=JA.view(-1)
J0=JA.clone()
dm=[]
m=[]
Gs=[]
rbm = EquivariantRBM(biadj_dat['cross'], Bond(Sn_dat[2], ['A1','E']))
for _ in range(N):
\trbm.set_J(J)
\tG=GSD(rbm.prob_src(split=True)).view(-1,1)
\tGs.append(torch.cat((JA,G),-1))
\tm.append(torch.cat((J0, mlrg.moderator(J).view(-1,1)),1))
\tdm.append(torch.cat((J0, mlrg.moderator.gradC(J).norm(dim=-1).view(-1,1)),1))
\tJ=mlrg.moderator.RGforward(J)
dm=torch.cat(dm).view(N,-1,2)
m=torch.cat(m).view(N,-1,2)
Gs=torch.cat(Gs).view(N,-1,2)\
\>", "ExternalLanguage",
