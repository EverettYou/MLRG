 FormBox[
  RowBox[{
   SubscriptBox["M", 
    RowBox[{
     SubscriptBox["x", "2"], 
     SubscriptBox["x", "4"]}]], "=", 
   RowBox[{
    UnderscriptBox["\[Sum]", 
     RowBox[{
      SubscriptBox["x", "1"], ",", 
      SubscriptBox["x", "3"]}]], 
    RowBox[{
     SubscriptBox["\[DoubleStruckCapitalT]", 
      RowBox[{
       SubscriptBox["x", "1"], 
