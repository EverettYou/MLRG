  RowBox[{
   RowBox[{"\[LeftBracketingBar]", "G", "\[RightBracketingBar]"}], "=", 
   RowBox[{
    FractionBox[
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"Tr", " ", "M"}], ")"}], "2"], 
     RowBox[{"Tr", " ", 
